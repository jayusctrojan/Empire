  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Empire Desktop default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "http:default",
    "shell:default",
    "store:default",
    "os:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "log:default"
  ]
}
